  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "prettier": {
    "printWidth": 120
  },
  "scripts": {
    "start": "tsc --watch",
    "compile": "tsc &&  typedoc --out ./docs ./src/main.tsx",
