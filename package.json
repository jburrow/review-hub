    "react-dom": "16.x || 17.x || 18.x"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.0.9",
    "file-saver": "^2.0.5",
    "jszip": "^3.10.0",
    "moment": "^2.29.3",
    "monaco-editor": "^0.26.1",
    "monaco-review": "0.0.30",
    "react-grid-layout": "^1.2.0",
    "react-with-styles": "^4.1.0",
    "rooks": "5.11.2",
    "stream": "0.0.2",
    "zip-js": "0.0.2"
  },
  "devDependencies": {
    "@babel/runtime": "^7.12.5",
    "@babel/types": "^7.12.12",
    "@types/jest": "^26.0.23",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "@types/react-grid-layout": "^1.1.1",
    "@types/uuid": "^8.3.0",
    "coveralls": "^3.1.0",
    "jest": "^27.0.4",
    "ts-jest": "^27.0.3",
    "typedoc": "^0.22.17",
    "typescript": "^4.7.3"
  }
