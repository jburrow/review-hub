  "description": "ReactJs application, components and reducers for a web-based code review tool",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "prettier": {
    "printWidth": 120
  },
  "scripts": {
    "start": "tsc --watch",
    "compile": "tsc &&  typedoc --out ./docs ./src/index.ts",
    "doc": "typedoc --out ./docs ./src/index.ts",
    "test": "jest --coverage",
    "testpub": "jest --coverage --coverageReporters=text-lcov | coveralls"
  },
  "author": "James Burrow",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jburrow/review-hub.git"
  },
  "peerDependencies": {
